IFS="$(printf '\n\t')"; IFS=${IFS%?}

for NETDEV in $(route -n -A "inet") ''; do
  case ${NETDEV} in
    '0.0.0.0 '*) break;;
  esac
done
test -n "${NETDEV}" && printf '%s\n' "${NETDEV##* }"